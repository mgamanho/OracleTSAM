		TUXDIR="/home/oracle/tuxHome/tuxedo12.2.2.0.0"
		APPDIR="@APPDIR@"
		ENVFILE="@APPDIR@/ENVFILE"
		TLOGDEVICE="@APPDIR@/TLOG"
		TLOGNAME=TLOG
		TUXCONFIG="@APPDIR@/tuxconfig"

